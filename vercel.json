{
    "version": 2,
    "builds": [
        {
            "src": "./server.js",
            "use": "@vercel/node"
        },
        {
            "src": "dist/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server.js"
        },
        {
            "src": "/",
            "dest": "dist/src/"
        },
        {
            "src": "/(.+)",
            "dest": "dist/$1"
        },
        {
            "src": "/services",
            "dest": "services/"
        }
    ]
}