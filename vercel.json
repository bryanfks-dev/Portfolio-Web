{
    "version": 2,
    "builds": [
        {
            "src": "./server.js",
            "use": "@vercel/node"
        },
        {
            "src": "dist/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/services/(.*)",
            "dest": "server.js"
        },
        {
            "src": "/",
            "dest": "dist/src/home.html"
        },
        {
            "src": "/projects",
            "dest": "dist/src/projects.html"
        },
        {
            "src": "/aboutweb",
            "dest": "dist/src/aboutweb.html"
        },
        {
            "src": "/resume",
            "dest": "dist/docs/"
        },
        {
            "src": "/(.+)",
            "dest": "dist/$1"
        }
    ]
}