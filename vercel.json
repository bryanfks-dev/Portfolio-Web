{
    "version": 2,
    "builds": [
        {
            "src": "./server.js",
            "use": "@vercel/node"
        },
        {
            "src": "dist/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/services/(.*)",
            "dest": "server.js"
        },
        {
            "src": "/dist/src/(.*)",
            "dest": "server.js"
        },
        {
            "src": "/dist/partials/(.*)",
            "dest": "server.js"
        },
        {
            "src": "/dist/docs/(.*)",
            "dest": "server.js"
        }
    ]
}